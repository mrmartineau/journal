{
  "name": "journal",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev --port 6400",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "supabase": "supabase",
    "supabase:types": "npx supabase gen types typescript --project-id \"ksnfvoowoksouhunfcve\" --schema public > src/types/supabase.ts"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.0.1",
    "@sveltejs/kit": "^1.16.3",
    "@types/autosize": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-svelte": "^2.28.0",
    "postcss": "^8.4.23",
    "postcss-import": "^15.1.0",
    "postcss-import-ext-glob": "^2.1.1",
    "postcss-nested": "^6.0.1",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.0",
    "supabase": "^1.57.5",
    "svelte": "^3.59.1",
    "svelte-check": "^3.3.2",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.5",
    "vitest": "^0.25.8"
  },
  "type": "module",
  "dependencies": {
    "@formdata-helper/base": "^1.1.0",
    "@supabase/auth-helpers-sveltekit": "^0.9.4",
    "@supabase/supabase-js": "^2.21.0",
    "autosize": "^6.0.1",
    "svelte-markdown": "^0.2.3"
  }
}
